        "express": "^4.17.1",
        "knex": "^0.21.17",
        "pg": "^8.5.1",
        "redoc-cli": "^0.10.2",
        "swagger-jsdoc": "^6.0.6"
    },
    "devDependencies": {
        "nodemon": "^2.0.7"
    }
}
